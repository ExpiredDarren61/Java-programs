import java.util.Scanner;
public class Main {

    public static int square(int a, int b) {
        int square = 1;
        for (int i = 1; i <= b; i++) {
            square *= a;
        }
        return square;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int x = sc.nextInt();
        System.out.print("Enter a number: ");
        int n = sc.nextInt();
        System.out.println(square(x,n));
    }
}
